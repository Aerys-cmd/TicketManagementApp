@page
@model TicketManagementApp.Pages.Ticket.CompletedTicketsModel
@{
}
<table border="1" width="100%">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Description</th>
            <th>Assignee</th>
            <th>Closed Date</th>

        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.ViewModels.Count; i++)
        {
            <tr>
                <td>
                    <label>@Model.ViewModels[i].Subject</label>
                </td>
                <td>
                    <label>@Model.ViewModels[i].Description</label>
                </td>
                <td>
                    <label>@Model.ViewModels[i].Assignee</label>
                </td>
                <td>
                    <label>@Model.ViewModels[i].CompletedDate</label>
                </td>
            </tr>
        }
    </tbody>
</table>
